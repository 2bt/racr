% Program for ad hoc testing from command line.
% Can be changed arbitrary; is not part of the test-suite.

Procedure fibonacci(Var n:Integer):Integer Begin
	Var result:Integer;
	If (n > 1) 
		Then result := fibonacci(n-1) + fibonacci(n-2);
	Else
		result := 1;
	Fi;
	Return result;
End;
                      
Procedure main() Begin
	Var choice:Boolean;
	Var n:Integer;
	Read choice;
	Read n;
	
	If choice Then
		Write fibonacci(n);
	Else
		Var a1:Integer;
		Var a2:Integer;
		Var result:Integer;
		a1 := 1;
		a2 := 0;
		result := 0;
		While n # 0 Do
			result := a1 + a2;
			a2 := a1;
			a1 := result;
			n := n - 1;
		Od;
		Write result;
	Fi;
End;
